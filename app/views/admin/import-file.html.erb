            <!-- Start right Content here -->
            <!-- ============================================================== -->
            
            <div class="content-page">
                <!-- Start content -->
                <div class="content">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="card-box">
                                    <div id="custom-width-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="custom-width-modalLabel" aria-hidden="true" style="display: none;">
                                    <div class="modal-dialog" style="width:55%;">

                                    <!-- START MODAL CONTENT -->
                                        <div class="modal-content">
                                         <%= form_for(:import, :url => {:controller => 'admin', :action => 'import_job', :class => 'modal-content'}) do |f| %>
                                            <div class="modal-header">
                                                <a type="button" class="close" data-dismiss="modal" aria-hidden="true">×</a>
                                                <center>
                                                     <h4 class="modal-title" id="custom-width-modalLabel">Import File csv to your database</h4>
                                                </center>
                                               
                                            </div>
                                             <div class="modal-body">
                                                <h4>Job CSV</h4>
                                                <%= f.file_field :file, :class=>"form-control", :required => true, :placeholder => "Enter your File csv" %>
                                            </div>                    
                                            <div class="modal-footer">
                                                <a type="button" class="btn btn-danger" data-dismiss="modal">Close</a>
                                                <%= f.submit "Agree", :class => "btn btn-primary" %>
                                            </div>
                                        <% end %><!-- /.modal-content -->
                                        </div>
                                        <!--  END MODAL CONTENT -->
                                     </div><!-- /.modal-dialog -->
                                    </div><!-- /.modal -->
                                    <!-- END ADD DISPOSABLE EMAIL -->
                                    <div class="table-rep-plugin">
                                        <div class="table-responsive" data-pattern="priority-columns">
                                            <% if flash[:import_job_successfully] %>
                                                <div class="alert alert-success">
                                                <center>
                                                    <%= flash[:import_job_successfully] %>
                                                </center>
                                                </div>
                                            <% end %>
                                            <table id="tech-companies-1" class="table  table-striped">
                                                <thead>
                                                        <tr>
                                                            <th>ID</th>
                                                            <th data-priority="3">Admin ID</th>
                                                            <th data-priority="6">Title Job</th>
                                                            <th data-priority="1">Type Job</th>
                                                            <th data-priority="1">Vacancies</th>
                                                            <th data-priority="1">Phone Number</th>
                                                            <th data-priority="1">Company Name</th>
                                                            <th data-priority="1">Vacancies Start</th>
                                                            <th data-priority="1">Employer Real</th>
                                                            <th data-priority="6">Action</th>
                                                        </tr>
                                                    
                                                </thead>

                                                <tbody>
                                                <% @jobs = Job.paginate(:per_page => 10, :page => params[:page]) %>
                                                <% @jobs.each do |job| %> 
                                                    <tr>
                                                        <th><span class="co-name">
                                                            <%= job.id %>    
                                                          </span>
                                                        </th>
                                                        <td> 
                                                          <%= job.user_id %> 
                                                        </td>
                                                        <td> 
                                                          <%= job.title %>
                                                        </td>
                                                        <td> 
                                                          <%= job.type_job %>
                                                        </td>
                                                        <td> 
                                                          <%= job.vacancies %>
                                                        </td>
                                                        <td> 
                                                          <%= job.phone_number %>
                                                        </td>
                                                        <td> 
                                                          <%= job.company_name %>
                                                        </td>
                                                        <td> 
                                                          <%= job.vacancies_start %>
                                                        </td>
                                                        <td> 
                                                          <%= job.employer_real %>
                                                        </td>
                                                         <td> 
                                                               <%= link_to "Delete", {controller: "admin", action: "delete_job_employer", id: job.id }, class: "btn btn-danger waves-effect w-md waves-light m-b-5" %>
                                                        </td>
                                                    </tr> 
                                                <% end %>     
                                                </tbody>
                                            </table>
                                            <center>
                                                <a class="btn btn-primary waves-effect waves-light" data-toggle="modal" data-target="#custom-width-modal">Import Job To Database.</a>
                                            </center>
                                            <p><%= will_paginate @jobs %></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- End row -->

                    </div> <!-- container -->

                </div> <!-- content -->

                <footer class="footer">
                    2017 © Parttimebee.
                </footer>

            </div>

            
            <!-- End content-page -->


            <!-- ============================================================== -->
            <!-- End Right content here -->
