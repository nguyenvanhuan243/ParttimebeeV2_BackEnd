                <div class="container">
                    <h1>All Jobs</h1>   
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation">
                            <a href="#applied-jobs" aria-controls="applied-jobs" role="tab" data-toggle="tab">Applied jobs
                                <span><%= Applied.where(status_job: "applied", parttimer_id: current_user[:id]).count %>
                                </span>
                            </a>
                        </li>
                        <li role="presentation"><a href="#started-jobs" aria-controls="started-jobs" role="tab" data-toggle="tab">Awarded jobs<span><%= Applied.where(status_job: "awarded", parttimer_id: current_user.id).count%></span></a></li>
