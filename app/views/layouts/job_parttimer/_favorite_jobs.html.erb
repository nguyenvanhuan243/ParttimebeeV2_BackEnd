                        <span><%=Applied.where(status_job: "completed", parttimer_id: current_user.id).count %></span></a></li>
                        <% @count_favorite_job = 0%>
                        <% Job.all.each do |job|%>
                          <% if Favoritejob.where(parttimer_id: current_user.id, job_id: job.id).count.positive?%>
                              <% @count_favorite_job += 1%>
                          <% end %>
                        <% end %>
                        <li role="presentation" class="active"><a href="#favorite-jobs" aria-controls="completed-jobs" role="tab" data-toggle="tab">Favorite jobs
