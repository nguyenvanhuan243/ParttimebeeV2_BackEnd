                                                <span class="time-add pull-right"><%=@get_min.to_i.abs%> minutes ago</span>
                                            <% end %>
                                            <% if @get_hour >= 1 %>
                                                <span class="time-add pull-right"><%=@get_hour.to_i.abs%> hours ago</span>
                                            <% end %>
