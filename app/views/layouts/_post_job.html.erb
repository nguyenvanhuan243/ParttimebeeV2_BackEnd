                            <div class="rectangle-6"></div>
                            <div class="submit">
                                <a class="btn btn-primary btnPrevious">Back</a>
                                <button type="submit" class="btn btn-default waves-effect">Publish</button>
                            </div>
                    </div><!-- .auto-container -->
                </div><!-- .content -->
            </div><!-- .tab-5 -->
        </div>
    </div><!-- wapper -->
<% end %>
