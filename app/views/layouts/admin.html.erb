<!DOCTYPE html>
<html>
   <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
      <meta name="author" content="Coderthemes">
      <link rel="shortcut icon" href="assets/images/favicon.ico">
      <title>Parttimebee Admin</title>
      <%= stylesheet_link_tag 'admin/bootstrap.min' %>
      <%= stylesheet_link_tag 'admin/core' %>
      <%= stylesheet_link_tag 'admin/components' %>
      <%= stylesheet_link_tag 'admin/icons' %>
      <%= stylesheet_link_tag 'admin/pages' %>
      <%= stylesheet_link_tag 'admin/menu' %>
      <%= stylesheet_link_tag 'admin/responsive' %>
      <%= stylesheet_link_tag 'admin/morris' %>
      <%= stylesheet_link_tag 'admin/summernote' %>
      <%= stylesheet_link_tag 'admin/custombox.min' %>
   </head>
   <body class="fixed-left">
      <!-- Begin page -->
      <div id="wrapper">
         <!-- Top Bar Start -->
         <div class="topbar">
            <!-- LOGO -->
            <div class="topbar-left">
               <a href="/admin" class="logo"><span>Parttimebee<span></span></span><i class="zmdi zmdi-layers"></i></a>
            </div>
            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
               <div class="container">
                  <!-- Page title -->
                  <ul class="nav navbar-nav navbar-left">
                     <li>
                        <button class="button-menu-mobile open-left">
                        <i class="zmdi zmdi-menu"></i>
                        </button>
                     </li>
                     <li>
                        <h4 class="page-title">SYSTEM ADMIN FOR PARTTIMEBEE</h4>
                     </li>
                  </ul>
                  <!-- Right(Notification and Searchbox -->
                  <!--  -->
                  <ul class="nav navbar-nav navbar-right">
                     <!-- <li> -->
                     <!-- Notification -->
                     <!-- <div class="notification-box">
                        <ul class="list-inline m-b-0">
                            <li>
                                <a href="javascript:void(0);" class="right-bar-toggle">
                                    <i class="zmdi zmdi-notifications-none"></i>
                                </a>
                                <div class="noti-dot">
                                    <span class="dot"></span>
                                    <span class="pulse"></span>
                                </div>
                            </li>
                        </ul>
                        </div> -->
                     <!-- End Notification bar -->
                     <!-- </li> -->
                     <!-- <li class="hidden-xs">
                        <form role="search" class="app-search">
                            <input type="text" placeholder="Search..."
                                   class="form-control">
                            <a href=""><i class="fa fa-search"></i></a>
                        </form>
                        </li> -->
                  </ul>
               </div>
               <!-- end container -->
            </div>
            <!-- end navbar -->
         </div>
         <!-- Top Bar End -->
         <!-- ========== Left Sidebar Start ========== -->
         <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">
               <!-- User -->
               <div class="user-box">
                  <div class="user-img">
                     <%= image_tag "admin/users/avatar-1.jpg", class: "img-circle img-thumbnail img-responsive", alt: "user-img", title: "Mat Helme" %>
                     <div class="user-status offline"><i class="zmdi zmdi-dot-circle"></i></div>
                  </div>
                  <h5><a href="#">Admin System</a> </h5>
                  <ul class="list-inline">
                     <li>
                        <%= link_to " Logout", {controller: "adminlogin", action: 'logout'}, :class => "text-custom zmdi zmdi-power" %>
                     </li>
                  </ul>
               </div>
               <!-- End User -->
               <!--- Sidemenu -->
               <div id="sidebar-menu">
                  <ul>
                     <li>
                        <a href="/admin/manage-jobs-employer" class="waves-effect active"><i class="zmdi zmdi-view-dashboard"></i> <span> Dashboard </span> </a>
                     </li>
                     <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect"><i class="zmdi zmdi-view-list"></i> <span> User management </span> <span class="menu-arrow"></span></a>
                        <ul class="list-unstyled">
                            <li>
                               <a href="/admin/blocking">
                                    Blocking
                                </a>
                            </li>
                            <li>
                               <a href="/admin/user-detail">
                                    User Detail
                                </a>
                            </li>
                            <li>
                                <a href="/admin/blocking-email-list">
                                    Blocking Email List
                                </a>
                            </li>
                            <li>
                                <a href="/admin/delete-user">
                                    User Delete
                                </a>
                            </li>
                            <li>
                                <a href="/admin/update-profile-special-account">
                                    Update profile special account
                                </a>
                            </li>
                        </ul>
                     </li>
                     <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect"><i class="zmdi zmdi-collection-text"></i><span> Send Notifications </span> </a>
                        <ul class="list-unstyled">
                           <li><a href="/admin/notification-all-member">Send All member</a></li>
                        </ul>
                     </li>
                     <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect"><i class="zmdi zmdi-view-list"></i> <span> Manage Jobs </span> <span class="menu-arrow"></span></a>
                        <ul class="list-unstyled">
                           <li><a href="/admin/manage-jobs-employer">Delete jobs Employer</a></li>
                           <li><a href="/admin/manage-merge-the-job">Merge the jobs</a></li>
                           <li><a href="/admin/manage-jobs-special-account">Job of special account</a></li>
                        </ul>
                     </li>
                     <li>
                        <a href="/admin/category-job" class="waves-effect"><i class="zmdi zmdi-email"></i><span class="label label-purple pull-right"></span><span> Category Job </span></a>
                     </li>
                     <li>
                        <a href="/admin/quota-job" class="waves-effect"><i class="zmdi zmdi-email"></i><span class="label label-purple pull-right"></span><span> Quota Apply Jobs </span></a>
                     </li>
                     <li>
                        <a href="/admin/import-file" class="waves-effect"><i class="fa fa-file-excel-o"></i><span class="label label-purple pull-right"></span><span> Import File Jobs </span></a>
                     </li>
                  </ul>
                  <div class="clearfix"></div>
               </div>
               <!-- Sidebar -->
               <div class="clearfix"></div>
            </div>
         </div>
         <!-- Left Sidebar End -->
         <!-- Content Load in here -->
         <%= yield %>
         <!-- Right Sidebar -->
         <div class="side-bar right-bar">
            <a href="javascript:void(0);" class="right-bar-toggle">
            <i class="zmdi zmdi-close-circle-o"></i>
            </a>
            <h4 class="">Notifications</h4>
            <div class="notification-list nicescroll">
               <ul class="list-group list-no-border user-list">
                  <li class="list-group-item">
                     <a href="#" class="user-list-item">
                        <div class="avatar">
                           <%= image_tag "admin/users/avatar-2.jpg", alt: "" %>
                        </div>
                        <div class="user-desc">
                           <span class="name">Michael Zenaty</span>
                           <span class="desc">There are new settings available</span>
                           <span class="time">2 hours ago</span>
                        </div>
                     </a>
                  </li>
                  <li class="list-group-item">
                     <a href="#" class="user-list-item">
                        <div class="icon bg-info">
                           <i class="zmdi zmdi-account"></i>
                        </div>
                        <div class="user-desc">
                           <span class="name">New Signup</span>
                           <span class="desc">There are new settings available</span>
                           <span class="time">5 hours ago</span>
                        </div>
                     </a>
                  </li>
                  <li class="list-group-item">
                     <a href="#" class="user-list-item">
                        <div class="icon bg-pink">
                           <i class="zmdi zmdi-comment"></i>
                        </div>
                        <div class="user-desc">
                           <span class="name">New Message received</span>
                           <span class="desc">There are new settings available</span>
                           <span class="time">1 day ago</span>
                        </div>
                     </a>
                  </li>
                  <li class="list-group-item active">
                     <a href="#" class="user-list-item">
                        <div class="avatar">
                           <%= image_tag "admin/users/avatar-3.jpg", alt: "" %>
                        </div>
                        <div class="user-desc">
                           <span class="name">James Anderson</span>
                           <span class="desc">There are new settings available</span>
                           <span class="time">2 days ago</span>
                        </div>
                     </a>
                  </li>
                  <li class="list-group-item active">
                     <a href="#" class="user-list-item">
                        <div class="icon bg-warning">
                           <i class="zmdi zmdi-settings"></i>
                        </div>
                        <div class="user-desc">
                           <span class="name">Settings</span>
                           <span class="desc">There are new settings available</span>
                           <span class="time">1 day ago</span>
                        </div>
                     </a>
                  </li>
               </ul>
            </div>
         </div>
         <!-- /Right-bar -->
      </div>
      <!-- END wrapper -->
      <%= javascript_include_tag "admin/jquery.min" %>
      <%= javascript_include_tag "admin/bootstrap.min" %>
      <%= javascript_include_tag "admin/detect" %>
      <%= javascript_include_tag "admin/fastclick" %>
      <%= javascript_include_tag "admin/jquery.blockUI" %>
      <%= javascript_include_tag "admin/waves" %>
      <%= javascript_include_tag "admin/jquery.nicescroll" %>
      <%= javascript_include_tag "admin/jquery.slimscroll" %>
      <%= javascript_include_tag "admin/jquery.scrollTo.min" %>
      <%= javascript_include_tag "admin/jquery.knob" %>
      <%= javascript_include_tag "admin/morris.min" %>
      <%= javascript_include_tag "admin/raphael-min" %>
      <%= javascript_include_tag "admin/jquery.dashboard" %>
      <%= javascript_include_tag "admin/jquery.core" %>
      <%= javascript_include_tag "admin/jquery.app" %>
      <%= javascript_include_tag "admin/modernizr.min" %>
      <%= javascript_include_tag "admin/summernote.min" %>
      <%= javascript_include_tag "admin/custombox.min" %>
      <%= javascript_include_tag "admin/legacy.min" %>
      <!-- Note that: Because <script></script> can't load in file.html.erb   -->
      <%= javascript_include_tag "admin/huannguyen" %>
   </body>
</html>