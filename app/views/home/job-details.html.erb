<body class="employer-published-job-applications job-detail register-01">
    <div class="wapper"> 
        <%= render "layouts/header"%>   
        <div class="all-jobs">
            <div class="auto-container box-all-jobs">
                <div class="container">
                    <% @job_detail = Job.find(params[:id])%>
                    <% job = Job.find(params[:id] )%>
                    <div class="row">
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12 item-info">            
                            <div class="option">
                                <h1 class="title">
                                    <a href="#" title="Front-end Developer"><%= job[:title]%></a>
                                    <% @get_hour = Time.now.hour - job[:created_at].hour %>
                                    <% @get_min = Time.now.min - job[:created_at].min%>
                                    <% if @get_hour < 1 %>
                                        <span class="time"><%=@get_min.to_i.abs%> minutes ago</span>
                                    <% end %>
                                    <% if @get_hour > 1 %>
                                        <span class="time"><%=@get_hour.to_i.abs%> hours ago</span>
                                    <% end %>
                                </h1>
                                <div class="option-top">
                                    <div class="item-note"> 
                                        <div class="promotion-flags">
                                            <% if job[:on_the_spot] == "yes" %>
                                                <a href="#" class="btn btn-spot waves-effect" title="On The Spot">On The Spot</a>
                                            <% end %>
                                            <% if job[:commission] == "yes"%>
                                                <a href="#" class="btn btn-commission waves-effect" title="Commission">Commission</a>
                                            <% end %>
                                        </div><!-- .promotion-flags -->
                                    </div>
                                    <div class="tag-location">
                                        <div class="tag-link">  
                                            <a href="#" title="Technology">
                                                <%= job[:category_job]%>
                                            </a>
                                        </div>
                                        <div class="location-link">
                                            <a class="fancybox" href="#inline2">
                                                <%= job[:state]%>
                                            </a>
                                        </div>
                                    </div><!-- .tag-location -->
                                </div><!-- .option top -->
                                <div class="option-bottom">
                                    <div class="row">
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">      
                                            <div class="time-work">
                                                <span>Working period: <span class="month">
                                                    <!-- 1 Jun - 30 Jun -->
                                                    <%= job[:working_date_from].to_date.strftime("%d %B")%> - 
                                                    <%= job[:working_date_to].to_date.strftime("%d %B")%>
                                
                                                </span></span>
                                                <span>Working Hour: <span class="month"><%=job[:working_hour_from]%> am - <%= job[:working_hour_to]%> pm</span></span> 
                                            </div><!-- .time work -->
                                            <div class="time-work">
                                                <span>Language: <span class="font-bold">
                                                <% if job[:language_Bahasa_Malaysia] == "1" %>
                                                    Bahasa Malaysia,
                                                <% end %>
                                                <% if job[:language_Chinese] == "1" %>
                                                    Chinese,
                                                <% end %>
                                                <% if job[:language_English] == "1" %>
                                                    English,
                                                <% end %>
                                                <% if job[:language_Tamin] == "1" %>
                                                    Cantonese,
                                                <% end %>
                                                <% if job[:language_Cantonese] == "1" %>
                                                    English,
                                                <% end %>
                                                <% if job[:language_Hokkien] == "1" %>
                                                    Hokkien
                                                <% end %>
                                                  </span></span>
                                            </div><!-- .time work -->
                                            <div class="package-link">
                                                <a href="#">Open to <span class="font-bold"><%= job[:availablefor]%></span></a>
                                                <a href="#"><span class="font-bold"><%= job[:vacancies]%></span> vacancies available</a>
                                                <a href="#">Pay <span class="font-bold">
                                                    <%= job[:payment]%> 
                                                    </span></a>
                                            </div><!-- .package -->
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 price">    
                                            <span class="pull-right">RM<%=job[:salary]%>/<%= job[:per_day] %> </span>
                                        </div>
                                    </div><!-- .row -->
                                </div><!-- .option bottom -->
                            </div><!-- .option -->
                        </div><!-- .item-info -->
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 right-banner">
                            <div class="banner-use">
                                <% @user = User.find(job.user_id)%>
                                <% if Profile.where(user_id: @user.id).count.positive?%>
                                    <% @profile = Profile.where(user_id: @user.id).first%>
                                    <% if @profile.company_logo_file_name != nil%>
                                        <%= image_tag @profile.company_logo.url, alt: "use", :class => "avatar-custom" %>
                                    <% else %>
                                        <%= image_tag "default-avatar.png", alt: "use", :class => "avatar-custom" %>
                                    <% end %>
                                <% else %>
                                        <%= image_tag "default-avatar.png", alt: "use", :class => "avatar-custom" %>
                                <% end %>
                                <div class="use-info">
                                    <p><%= @user.full_name %></p>
                                    <span><%= @user.jobs.count%> jobs available</span>
                                    <!-- <a class="waves-effect fancybox" href="#inline1">See Full Detail</a> -->
                                    <a class="waves-effect fancybox" href="/05-profile-00-01/<%=@user.id%>">See Full Detail</a>

                                </div>
                                <div id="inline1" style="display: none;">
                                    <div class="profile-popup">
                                    <% @user = User.first%>
                                        <div class="popup-content">
                                            <div class="popup-image">
                                                <% if @user.url_avatar.blank?%>
                                                    <%= image_tag "default-avatar.png", alt: "use", :class => "avatar-custom" %>
                                                <% else %>
                                                    <%= image_tag @user.url_avatar, alt: "use", :class => "avatar-custom" %>
                                                <% end %>
                                            </div>
                                            <div class="popup-info">
                                                <h4>User Name</h4>
                                                <span><%= @user.nationality%></span>
                                                <span>Member since <%= @user[:created_at].strftime("%d %b %Y")%></span>
                                            </div>
                                            <div class="jobs-available">
                                                <span><%= @jobs_available %></span>
                                                <h5>Jobs available</h5>
                                            </div>
                                            <% if Profile.where(user_id: job[:user_id]).count > 0 %>
                                            <% @profile = Profile.where(user_id: job[:user_id]).first%>
                                            <div>
                                            <% unless @profile[:company_bio].blank?%>
                                                <%= @profile[:company_bio].html_safe%>
                                            <% end %>
                                            </div>
                                            <% end %>
                                            <div class="btn-popup">
                                                <a class="waves-effect" href="/05-profile-00-01/<%=@user.id%>">See Full Profile</a>
                                            </div>
                                        </div><!-- .popup body-->
                                    </div><!-- .popup -->
                                </div>
                            </div><!-- .banner-use -->
                            <div class="banner-people">
                                <p>Part-timers who view this job</p>
                                <% if current_user %>
                                  <% if Viewjob.where(parttimer_id: current_user.id, job_id: job.id).count < 1%>
                                    <% @view_job = Viewjob.new %>
                                    <% @view_job.parttimer_id = current_user.id%>
                                    <% @view_job.job_id = job.id %>
                                    <% @view_job.save %>
                                  <% end %>
                                <% end %>
                                <!-- Count views jobs -->
                                <% job_view = Job.find(job.id)%>
                                <% job_view.views = Viewjob.where(job_id: job.id).count%>
                                <% job_view.save %>
                                <%Viewjob.where(job_id: job.id).take(7).each do |job|%>
                                <% if User.find(job.parttimer_id).avatar_file_name == nil %>
                                  <%= image_tag "default-avatar.png", alt: "people", :class=> 'avatar-custom'%>
                                <% else %>
                                  <%= image_tag User.find(job.parttimer_id).avatar.url("medium"), alt: "people", :class=> 'avatar-custom '%>
                                <% end %>                             
                              <% end %>
                              <% if Viewjob.where(job_id: job.id).count > 7%>
                                    <span class="cover layer">+<%=Viewjob.where(job_id: job.id).count - 7%></span>
                                <% end %>
                            </div><!-- .people -->

                            <div class="btn-link">
                            <!-- Check if job of employer not real -->
                                <% if job.user_id == 1%>
                                    <ul>
                                    <% if current_user.nil? %>
                                       <li class="apply-now"><a class="waves-effect" href="/login">Apply Now</a></li>
                                    <% else %>
                                        <% if current_user[:type_user]=="parttimer"%>
                                            <% if current_user.verify_findjob_parttimer.zero?%>
                                              <li class="apply-now otp"><a class="waves-effect fancybox" href="#hiddenotp">Apply Now</a></li>
                                            <% else %>
                                              <% if Applied.where(job_id: job.id, parttimer_id: current_user.id).count.positive? %>
                                                <li class="apply-now">
                                                  <a class="waves-effect" href="#">Applied</a>
                                                </li>
                                              <% else %>
                                                <li class="apply-now" onclick= "notify_sms_not_employer()" >
